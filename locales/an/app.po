msgid "Off"
msgstr "Desactivau"

#. Item in settings that will open a new preference screen for configuring the
#. URL autocomplete behavior.
msgctxt "preference_subitem_autocomplete"
msgid "URL Autocomplete"
msgstr "Autocompletar URL"

#. Title of "switch" preference that enables/disables autocomplete
msgctxt "preference_switch_autocomplete"
msgid "Autocomplete"
msgstr "Autocompletar"

#. Preference title: URL autocomplete list that ships with Focus/Klar.
msgctxt "preference_category_autocomplete_preinstalled"
msgid "Default URL List"
msgstr "Lista URL per defecto"

#. Preference summary: URL autocomplete list that ships with Focus/Klar.
#, c-format
msgctxt "preference_autocomplete_preinstalled_summary"
msgid "Enable to have %1$s autocomplete over 450 popular URLs in the address bar."
msgstr ""
"Activar pa tener autocompletau de %1$s con 450 URLs populars en a barra "
"d'adrezas."

#. Preference category: Custom (user-defined) autocomplete URLs.
msgctxt "preference_category_autocomplete_custom_urls"
msgid "Custom URLs"
msgstr ""

#. Item in autocomplete settings that will open a new preference screen for
#. adding and removing custom autocomplete URLs
msgctxt "preference_autocomplete_subitem_customlist"
msgid "Custom URLs"
msgstr "URLs personalizadas"

